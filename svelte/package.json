{
  "name": "@boxyhq/svelte-ui",
  "description": "Svelte UI components from BoxyHQ",
  "version": "2.3.0",
  "type": "module",
  "keywords": [
    "svelte",
    "boxyhq",
    "sso",
    "enterprise-features",
    "mitosis",
    "component-library"
  ],
  "files": [
    "dist",
    "index.d.ts",
    "sso.d.ts"
  ],
  "types": "./index.d.ts",
  "exports": {
    "./sso": {
      "import": "./dist/sso.js",
      "require": "./dist/sso.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "1.5.0",
    "svelte": "3.59.1",
    "svelte-check": "3.4.3",
    "typescript": "5.1.3",
    "vite": "4.3.0"
  }
}
